
.xxu-card{
	border-radius: 7px;
	background: -webkit-linear-gradient(135deg, #FFD3A5 10%, #FD6585 100%); /* Safari 5.1 - 6.0 */ /* Opera 11.1 - 12.0 */ /* Firefox 3.6 - 15 */ 
	background: -webkit-linear-gradient(315deg, #FFD3A5 10%, #FD6585 100%); 
	background: linear-gradient(135deg, #FFD3A5 10%, #FD6585 100%);
	background-image: -webkit-linear-gradient(315deg, #FFD3A5 10%, #FD6585 100%);
	background-image: linear-gradient(135deg, #FFD3A5 10%, #FD6585 100%);
}
.u-cell{
	display: contents !important;
}
.u-cell-box {
	margin-bottom: 100rpx !important;
}

